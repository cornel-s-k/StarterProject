        </span><span><span class="hljs-keyword">const</span></span><span> map = </span><span><span class="hljs-title function_">createMap</span></span><span>(</span><span><span class="hljs-string">`map-<span class="hljs-subst">${index}</span></span></span><span>`, [story.</span><span><span class="hljs-property">lat</span></span><span>, story.</span><span><span class="hljs-property">lng</span></span><span>], </span><span><span class="hljs-number">13</span></span><span>);
        </span><span><span class="hljs-keyword">const</span></span><span> marker = L.</span><span><span class="hljs-title function_">marker</span></span><span>([story.</span><span><span class="hljs-property">lat</span></span><span>, story.</span><span><span class="hljs-property">lng</span></span><span>]).</span><span><span class="hljs-title function_">addTo</span></span><span>(map);
        marker.</span><span><span class="hljs-title function_">bindPopup</span></span><span>(</span><span><span class="hljs-string">`&lt;strong&gt;<span class="hljs-subst">${story.title}</span></span></span><span>&lt;/strong&gt;&lt;br&gt;</span><span><span class="hljs-subst">${story.description}</span></span><span>`);
      }
    });
  } </span><span><span class="hljs-keyword">catch</span></span><span> (error) {
    </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">error</span></span><span>(</span><span><span class="hljs-string">'Failed to load stories:'</span></span><span>, error);
    </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">querySelector</span></span><span>(</span><span><span class="hljs-string">'#story-list'</span></span><span>).</span><span><span class="hljs-property">innerHTML</span></span><span> = </span><span><span class="hljs-string">'&lt;p&gt;Gagal memuat cerita. Coba lagi nanti.&lt;/p&gt;'</span></span><span>;
  }
}
</span></span></code></div></div></pre>
<hr data-start="4654" data-end="4657" class="" style="">
<h3 data-start="4659" data-end="4678" class=""><strong data-start="4663" data-end="4678">Kesimpulan:</strong></h3>
<ul data-start="4679" data-end="4929">
<li data-start="4679" data-end="4763" class="" style="">
<p data-start="4681" data-end="4763" class="">Pastikan properti <code data-start="4699" data-end="4706">image</code>, <code data-start="4708" data-end="4713">lat</code>, dan <code data-start="4719" data-end="4724">lng</code> ada dan valid di setiap objek <code data-start="4755" data-end="4762">story</code>.</p>
</li>
<li data-start="4764" data-end="4867" class="" style="">
<p data-start="4766" data-end="4867" class="">Periksa apakah <code data-start="4781" data-end="4795">getStories()</code> mengembalikan data yang benar dan sesuai dengan format yang diharapkan.</p>
</li>
<li data-start="4868" data-end="4929" class="" style="">
<p data-start="4870" data-end="4929" class="">Gunakan fallback untuk gambar dan handle error dengan baik.</p>
